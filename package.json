{
  "name": "dj",
  "displayName": "dj",
  "description": "Dave's Journal Extension for VS Code",
  "publisher": "davehadley",
  "repository": {
    "type": "git",
    "url": "https://github.com/davehadley/dj-vscode"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dj.capture",
        "title": "DJ: Capture"
      },
      {
        "command": "dj.note",
        "title": "DJ: Note"
      },
      {
        "command": "dj.today",
        "title": "DJ: Today"
      },
      {
        "command": "dj.yesterday",
        "title": "DJ: Yesterday"
      }
    ],
    "keybindings": [
      {
        "command": "dj.capture",
        "key": "alt+."
      }
    ],
    "configuration": {
      "title": "DJ",
      "properties": {
        "dj.directory": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "Journal root directory where all notes will be stored."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "deploy": "vsce publish --yarn",
    "deploylocal": "python scripts/deploylocal.py"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vsce": "^1.85.0",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "moment": "^2.29.1"
  }
}
